	["Yellow"] = Color3.fromRGB(255, 170, 0), -- Yellow
	["Purple"] = Color3.fromRGB(170, 0, 127), -- Purple
	["Pink"] = Color3.fromRGB(255, 85, 255), -- Pink
};


