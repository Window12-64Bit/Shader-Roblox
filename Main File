for i, v in pairs(ButtonList:GetChildren()) do
	if v:IsA("TextButton") then
		-- Save the exact size it started with
		local originalSize = v.Size 
		-- Create a size just slightly bigger (add 5-10 pixels)
		local hoverSize = UDim2.new(originalSize.X.Scale, originalSize.X.Offset + 10, originalSize.Y.Scale, originalSize.Y.Offset + 5)

		v.MouseEnter:Connect(function()
			ts:Create(v, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {Size = hoverSize}):Play()
			SoundEnter:Play()
		end)
		

		v.MouseLeave:Connect(function()
			ts:Create(v, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {Size = originalSize}):Play()
		end)
	end
end

local Ui_Stroke = Instance.new("UIStroke")
UIListLayout.Wraps = true
HText.Font = Enum.Font.Kalam

for i, v in pairs(ButtonList:GetChildren()) do
	if v:IsA("TextButton") then
		local Cloned = Ui_Stroke:Clone()
		Cloned.Parent = v
		Cloned.Color = Color3.fromRGB(255, 255, 255)
		Cloned.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
	end
end




local function GlitchButton(Color: Color3)
	local CCE = Instance.new("ColorCorrectionEffect")
	if game.Lighting:FindFirstChild("G!E") then
		game.Lighting:WaitForChild("G!E"):Destroy()
	end
	CCE.Name = "G!E"
	CCE.Brightness = -555555
	CCE.Contrast = -5555555
	CCE.Saturation = -1
	local succ, err = pcall(function()
		CCE.TintColor = Color
	end)
	if succ then
		warn("Color" .. CCE.Name .. "Injected")
	else
		warn("Error Msg: " .. err)
	end
	CCE.Parent = game.Lighting
end

local function Clear(label)
	for i, v in pairs(game.Lighting:GetChildren()) do
		if v.Name == "G!E" then
			v:Destroy()
			label.Text = "Please wait..."
			task.wait(0.05)
		end
	end
	label.Text = "Clear Gie > Default"
end


local function EagleSightEffect()
	local Cc = Instance.new("ColorCorrectionEffect")
	local Bloom = Instance.new("BloomEffect")
	local Blur = Instance.new("BlurEffect")
	if game.Lighting:FindFirstChild("G!E") then
		game.Lighting:WaitForChild("G!E"):Destroy()
	end
	Cc.Contrast = 1.4
	Cc.Brightness = -0.2
	Bloom.Intensity = 1
	Bloom.Size = 46
	Blur.Size = 4
	Cc.Parent = game.Lighting
	Bloom.Parent = game.Lighting
	Blur.Parent =game.Lighting
	Cc.Name = "G!E"
	Bloom.Name = "G!E"
	Blur.Name = "G!E"
end



local function GreatEyeEffect()
	local Cc = Instance.new("ColorCorrectionEffect")
	local Bloom = Instance.new("BloomEffect")
	local Blur = Instance.new("BlurEffect")
	if game.Lighting:FindFirstChild("G!E") then
		game.Lighting:WaitForChild("G!E"):Destroy()
	end
	Cc.Contrast = 0.5
	Cc.Brightness = -0.3
	Blur.Size = 3
	Bloom.Enabled = true
	Bloom.Intensity = 50
	Bloom.Size = 20
	Cc.Parent = game.Lighting
	Bloom.Parent =  game.Lighting
	Blur.Parent =  game.Lighting
	Cc.Name = "G!E"
	Bloom.Name = "G!E"
	Blur.Name = "G!E"
end


local function PotatoEffect()
	game.Lighting.GlobalShadows= false
	for i, v in pairs(game.Lighting:GetChildren()) do
		if v:IsA("SunRaysEffect") or v:IsA("DepthOfFieldEffect") or v:IsA("ColorCorrectionEffect") or v:IsA("BlurEffect") or v:IsA("BloomEffect") then
			v:Destroy()
		end
	end
end

local function Animes()
	game.Lighting.GlobalShadows= true
	local Cc = Instance.new("ColorCorrectionEffect")
	if game.Lighting:FindFirstChild("G!E") then
		game.Lighting:WaitForChild("G!E"):Destroy()
	end
	Cc.Saturation = 1
	Cc.Brightness = -0.1
	Cc.Contrast = 1 
	Cc.Name = "G!E"
	Cc.Parent = game.Lighting
end


CrazyGlitch.MouseButton1Click:Connect(function()
	SoundClick:Play()
	if CGTextColor.Text ~= "" then
		if CGTextColor.Text == "red" then
			GlitchButton(ColorTable["Red"])
		elseif CGTextColor.Text == "blue" then
			GlitchButton(ColorTable["Blue"])
		elseif CGTextColor.Text == "green" then
			GlitchButton(ColorTable["Green"])
		elseif CGTextColor.Text == "yellow" then
			GlitchButton(ColorTable["Yellow"])
		elseif CGTextColor.Text == "purple" then
			GlitchButton(ColorTable["Purple"])
		elseif CGTextColor.Text == "pink" then
			GlitchButton(ColorTable["Pink"])
		else
			GlitchButton(Color3.fromRGB(255,255,255))
		end
	end
end)

GreatEye.MouseButton1Click:Connect(function()
	notify("Notification", "Effect Activated", "OKAY")
	SoundClick:Play()
	GreatEyeEffect()
end)

EagleSight.MouseButton1Click:Connect(function()
	notify("Notification", "Effect Activated", "OKAY")
	SoundClick:Play()
	EagleSightEffect()
end)

Potato.MouseButton1Click:Connect(function()
	notify("Notification", "Effect Activated", "OKAY")
	SoundClick:Play()
	PotatoEffect()
end)

Anime.MouseButton1Click:Connect(function()
	notify("Notification", "Effect Activated", "OKAY")
	SoundClick:Play()
	Animes()
end)

ClearGiE.MouseButton1Click:Connect(function()
	notify("Notification", "Effect Removed", "OKAY")
	SoundClick:Play()
	Clear(ClearGiE)
end)




